    <ApplicationManifest>app.manifest</ApplicationManifest>
    <PublishReadyToRun>true</PublishReadyToRun>
    <ApplicationIcon>icon-broom.ico</ApplicationIcon>
    <StartupObject>ClearFileTemp.Program</StartupObject>
    <Authors>Isaias D A</Authors>
    <Description>É uma aplicação que limpa as pasta temporárias do sistema e local.</Description>
    <PackageIcon>c_icon.png</PackageIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Include="icon\c_icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>
